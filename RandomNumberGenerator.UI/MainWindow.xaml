<Window x:Class="RandomNumberGenerator.UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RandomNumberGenerator.UI"
        xmlns:vw="clr-namespace:RandomNumberGenerator.UI.Pages"
        mc:Ignorable="d"
        Title="Random number" Height="300" Width="500"
        ResizeMode="NoResize">
    <Grid>
        <vw:RandomNumberPage DataContext="{Binding RandomNumberViewModel}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch"/>
    </Grid>
</Window>
